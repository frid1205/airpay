<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="AirpayTestSuite" parallel="tests" thread-count="1" verbose="1">
	<parameter name="browser" value="chrome"/>
	<parameter name="headless" value="true"/>
	<!-- env airpay or vittel -->
	<parameter name="env" value="airpay"/>
  	<test name="Regresion test" preserve-order="true" group-by-instances="true">
	     <classes>
	    		<class name="TestCases.AuthenticationSignUpTest">
	    			<methods>
						<include name="A01_SignUpWithInvalidCredential"/>
	    			</methods>
	    		</class>
	    		<class name="TestCases.AuthenticationSignUpTest2">
	    			<methods>
						<include name="A02_SignUpWithValidCredential"/>
						<include name="A03_SecondStepSignUp"/>
						<include name="A04_ThirdStepSignUp"/>
						<include name="A05_FouthStepofSignUp"/>
						<include name="A06_VerifyEmailPage"/>
						
	    			</methods>
	    		</class>
	   			<class name="TestCases.AuthenticationSignInTest">
	    			<methods>
						<include name="B01_LoginWithInvalidCredential"/>
						<include name="B02_LoginWithUnverifiedUserAccount"/>
						<include name="B03_LoginWithVerifiedUserAccount"/>
	    			</methods>
	    		</class>
	    		
	    		<class name="TestCases.ForgetPasswordTest">
	    			<methods>
						<include name="C01_UnverifiedUserTryToForgetPassword"/>
						<include name="C02_ForgetPasswordWithInvalidEmailCredential"/>
						<include name="C03_ForgetPasswordWithValidEmailCredential"/>
	    			</methods>
	    		</class>
	    		
	    		<class name="TestCases.EditProfileTest">
	    			<methods>
						<include name="signIn"/>
						<include name="E01_AccessEditProfileBasicInformationTab"/>
						<include name="E02_AccessEditProfileSupportingDocumentTab"/>
						<include name="E03_LetMandatoryFieldIsEmptyBasicInfoTab"/>
						<include name="E04_LetMandatoryFieldIsEmptySupportingDocTab"/>
	    			</methods>
	    		</class>
	    		
	    		<class name="TestCases.UpdatePasswordTest">
	    			<methods>
	    				<include name="signIn"/>
						<include name="F01_UpdatePasswordWithIncorrectCurrentPassword"/>
						<include name="F02_UpdatePasswordWithNewPasswordAndCoPasswordAreDifferent"/>
						<include name="F03_UpdatePasswordWithCorrectInput"/>
	    			</methods>
	    		</class>
	    		
	    		<class name="TestCases.UploadContractTest">
	    			<methods>
	    				<include name="signIn"/>
						<include name="G01_UploadFile"/>
						<include name="G02_DownloadFile"/>
	    			</methods>
	    		</class>
	    		
	    		<class name="TestCases.DocumentationTest">
	    			<methods>
	    				<include name="signIn"/>
						<include name="H01_GettingStarted"/>
						<include name="H02_MerchantOnBoarding"/>
						<include name="H03_ServiceOnBoarding"/>
						<include name="H04_Integration"/>
						<include name="H05_Reporting"/>
						<include name="H06_FAQ"/>
	    			</methods>
	    		</class>
	    		
	    		<class name="TestCases.AddServiceTest">
	    			<methods>
	    				<include name="signIn"/>
						<include name="J01_FirstStepAddService"/>
						<include name="J02_SecondStepService"/>
						<include name="J03_ThirdStepService"/>
						<include name="J04_FourthStepService"/>
						<include name="J05_FifthStepService"/>
						<include name="J06_SixthStepService"/>
						
	    			</methods>
	    		</class>
	    		<!-- You need to execute service management test before list service test for approval -->
	    		<class name="TestCases.ServiceManagementTest">
	    			<methods>
	    				<include name="signIn"/>
	    				<include name="R01_ViewServiceManagement"/>
						<include name="R02_SearchServiceManagement"/>
						<include name="R03_ApproveService"/>
						<include name="R04_RejectService"/>
					</methods>
	    		</class>
	    		
	    		<class name="TestCases.ListServiceTest">
	    			<methods>
	    				<include name="signIn"/>
						<include name="K01_AccessListServicePage"/>
						<include name="K02_ActivateService"/>
						<include name="K03_DeactiveService"/>
						<include name="K04_EditServiceWithLetOneOrAllMandatoryFieldsAreEmpty"/>
						<include name="K05_EditServiceWithAllMandatoryFieldsAreFilled"/>
	    			</methods>
	    		</class>
	    		
	    		<class name="TestCases.InviteUserTest">
	    			<methods>
	    				<include name="signIn"/>
						<include name="N01_InviteUserWithAllMandatoryAreEmpty"/>
						<include name="N02_InviteTakenUsernameOrTakenEmailAddress"/>
						<include name="N03_InviteUserWithValidCredential"/>
	    			</methods>
	    		</class>
	    		
	    		<class name="TestCases.UserApprovalTest">
	    			<methods>
	    				<include name="signIn"/>
						<include name="O01_ClickOnApproveButtonRejectButtonWithoutSelectCheckbox"/>
						<include name="O02_RejectUser"/>
						<include name="O02_ApproveUser"/>
	    			</methods>
	    		</class>
	    </classes>  
  </test>
  
</suite>